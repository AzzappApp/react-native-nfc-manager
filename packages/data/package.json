{
  "name": "@azzapp/data",
  "version": "0.7.0-canary.16",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "generate-migration": "drizzle-kit generate --config ./src/drizzle.config.ts",
    "push-migration": "drizzle-kit push --config ./src/drizzle.config.ts",
    "introspect": "drizzle-kit introspect --config ./src/drizzle.config.ts",
    "clean": "rm -rf lib && rm -f tsconfig.build.tsbuildinfo && rm -rf node_modules",
    "studio": "drizzle-kit studio --config ./src/drizzle.config.ts"
  },
  "dependencies": {
    "@azzapp/i18n": "workspace:^",
    "@azzapp/shared": "workspace:^",
    "@paralleldrive/cuid2": "^2.2.2",
    "@planetscale/database": "^1.18.0",
    "drizzle-orm": "^0.33.0",
    "lodash": "^4.17.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@types/better-sqlite3": "7.6.10",
    "@types/lodash": "4.17.1",
    "@types/node-fetch": "2.6.11",
    "better-sqlite3": "^9.6.0",
    "drizzle-kit": "^0.24.2",
    "mysql2": "^3.9.7",
    "node-fetch": "2",
    "typescript": "^5.5.4"
  }
}

{
  "name": "@azzapp/data",
  "version": "0.5.5-canary.201",
  "private": true,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "build": "yarn compile && yarn definition",
    "dev": "yarn compile:watch & yarn definition:watch",
    "typecheck": "yarn definition",
    "generate-migration": "drizzle-kit generate:mysql --config ./src/drizzle.config.ts",
    "push-migration": "drizzle-kit push:mysql --config ./src/drizzle.config.ts",
    "introspect": "drizzle-kit introspect:mysql --config ./src/drizzle.config.ts",
    "definition": "tsc -p tsconfig.build.json",
    "definition:watch": "yarn definition --watch",
    "compile": "babel src --config-file ./babel.config.js -d lib --extensions '.ts,.tsx,.js,.jsx,.json'",
    "compile:watch": "yarn compile --watch",
    "clean": "rm -rf lib && rm -f tsconfig.build.tsbuildinfo && rm -rf node_modules",
    "studio": "drizzle-kit studio --config ./src/drizzle.config.ts"
  },
  "devScripts": [
    "compile:watch",
    "definition:watch"
  ],
  "dependencies": {
    "@azzapp/i18n": "0.5.5-canary.201",
    "@azzapp/shared": "0.5.5-canary.201",
    "@paralleldrive/cuid2": "^2.2.2",
    "@planetscale/database": "^1.16.0",
    "drizzle-orm": "^0.30.6"
  },
  "devDependencies": {
    "drizzle-kit": "0.20.14",
    "node-fetch": "^3",
    "mysql2": "^2.3.0"
  }
}

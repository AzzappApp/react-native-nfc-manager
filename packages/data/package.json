{
  "name": "@azzapp/data",
  "version": "0.0.4",
  "private": true,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    },
    "./domains": {
      "types": "./lib/domains/index.d.ts",
      "default": "./lib/domains/index.js"
    }
  },
  "scripts": {
    "build": "yarn compile && yarn definition",
    "dev": "yarn compile:watch & yarn definition:watch",
    "typecheck": "yarn definition",
    "generate-db-type": "prisma generate",
    "definition": "tsc -p tsconfig.build.json",
    "definition:watch": "yarn definition --watch",
    "compile": "babel src --config-file ./babel.config.js -d lib --extensions '.ts,.tsx,.js,.jsx,.json'",
    "compile:watch": "yarn compile --watch",
    "clean": "rm -rf lib && rm -f tsconfig.build.tsbuildinfo && rm -rf node_modules"
  },
  "devScripts": [
    "compile:watch",
    "definition:watch"
  ],
  "dependencies": {
    "@azzapp/auth": "*",
    "@azzapp/i18n": "*",
    "@azzapp/relay": "*",
    "@azzapp/shared": "*",
    "@paralleldrive/cuid2": "^2.2.0",
    "@planetscale/database": "^1.6.0",
    "@prisma/client": "^4.12.0",
    "dataloader": "^2.2.2",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.21.3",
    "kysely": "^0.24.2",
    "kysely-planetscale": "^1.3.0",
    "lodash": "^4.17.21",
    "graphql-tag": "^2.12.6",
    "@graphql-tools/schema": "^7.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.192",
    "prisma": "^4.12.0"
  }
}

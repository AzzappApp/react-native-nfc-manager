  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "generate-migration": "drizzle-kit generate --config ./src/drizzle.config.ts",

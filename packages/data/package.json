    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@types/better-sqlite3": "^7.6.11",
    "@types/lodash": "^4.17.10",
    "@types/node-fetch": "2.6.11",
    "better-sqlite3": "^9.6.0",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.17.0",
    "mysql2": "^3.11.3",
    "node-fetch": "2",
    "typescript": "^5.7.2"

{
  "name": "@azzapp/data",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    },
    "./domains": {
      "types": "./lib/domains/index.d.ts",
      "default": "./lib/domains/index.js"
    }
  },
  "scripts": {
    "build": "yarn compile && yarn definition",
    "dev": "yarn compile:watch & yarn definition:watch",
    "typecheck": "yarn definition",
    "generate-db-type": "prisma generate",
    "definition": "tsc -p tsconfig.build.json",
    "definition:watch": "yarn definition --watch",
    "compile": "babel src --config-file ./babel.config.js -d lib --extensions '.ts,.tsx,.js,.jsx,.json'",
    "compile:watch": "yarn compile --watch"
  },
  "devScripts": [
    "compile:watch",
    "definition:watch"
  ],
  "dependencies": {
    "@azzapp/relay": "*",
    "@azzapp/shared": "*",
    "@paralleldrive/cuid2": "^2.2.0",
    "@planetscale/database": "^1.5.0",
    "@prisma/client": "^4.8.1",
    "dataloader": "^2.1.0",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.20.1",
    "kysely": "^0.23.3",
    "kysely-planetscale": "^1.2.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "prisma": "^4.8.0"
  }
}

{
  "name": "@azzapp/service",
  "version": "2.0.0",
  "private": true,
  "exports": {
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "dev": "tsc --watch",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf lib && rm -f tsconfig.build.tsbuildinfo && rm -rf node_modules"
  },
  "devScripts": [
    "dev"
  ],
  "dependencies": {
    "@azzapp/data": "workspace:^",
    "@azzapp/shared": "workspace:^",
    "jose": "^6.0.10"
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.3"
  }
}

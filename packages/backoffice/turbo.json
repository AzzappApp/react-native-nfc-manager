{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/**",
        "src/public/**"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}

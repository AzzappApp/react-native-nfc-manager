{
  "name": "@azzapp/payment",
  "version": "1.4.2",
  "private": true,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "build": "yarn compile && yarn definition",
    "dev": "yarn compile:watch & yarn definition:watch",
    "typecheck": "yarn definition",
    "definition": "tsc -p tsconfig.build.json",
    "definition:watch": "yarn definition --watch",
    "compile": "babel src --config-file ./babel.config.js -d lib --extensions '.ts,.tsx,.js,.jsx,.json'",
    "compile:watch": "yarn compile --watch",
    "clean": "rm -rf lib && rm -f tsconfig.build.tsbuildinfo && rm -rf node_modules",
    "payment-api-client": "touch .env && node --env-file=.env ./scripts/generatePaymentApiClient.mjs"
  },
  "devScripts": [
    "compile:watch",
    "definition:watch"
  ],
  "dependencies": {
    "@azzapp/data": "workspace:^",
    "@azzapp/shared": "workspace:^",
    "@mlecoq/sales-tax": "2.14.1",
    "dayjs": "^1.11.13",
    "ms": "^2.1.3",
    "openapi-fetch": "^0.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@types/ms": "^0.7.34",
    "eslint": "^9.15.0",
    "openapi-typescript": "^7.4.3",
    "typescript": "^5.6.3"
  }
}

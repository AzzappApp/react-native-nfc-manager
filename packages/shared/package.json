  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@types/chroma-js": "^2.4.4",
    "@types/validator": "^13.12.2",
    "eslint": "^9.15.0",
    "typescript": "^5.6.3"
  }

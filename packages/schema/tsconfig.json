{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "paths": {
      "@azzapp/shared/*": ["../shared/src/*"],
      "@azzapp/data/*": ["../data/src/*"],
      "#testing/*": ["./testing/*"],
      "#*": ["./src/*"]
    }
  },
  "exclude": [
    "node_modules",
    "lib",
    "scripts",
    "tsconfig.json",
    "tsconfig.lib.json",
    "jest.config.js",
    "babel.config.js",
    "jest-e2e.config.js",
    "testing"
  ]
}

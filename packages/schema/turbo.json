{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["//#relay:build", "^build"],
      "outputs": ["lib/**"],
      "inputs": ["src/**"]
    },
    "typecheck": {
      "dependsOn": ["//#relay:build", "^build"],
      "inputs": ["src/**"]
    },
    "test": {
      "inputs": ["src/**"]
    }
  }
}



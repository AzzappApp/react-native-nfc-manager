{
  "name": "@azzapp/schema",
  "version": "1.6.1-rc.13",
  "private": true,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "generate-test-migration": "SQL_ENV=SQLITE drizzle-kit generate:sqlite --config ./testing/drizzle-test.config.ts",
    "build": "yarn compile && yarn definition",
    "dev": "yarn compile:watch & yarn definition:watch",
    "typecheck": "yarn definition",
    "definition": "tsc -p tsconfig.build.json",
    "definition:watch": "yarn definition --watch",
    "compile": "babel src --config-file ./babel.config.js -d lib --extensions '.ts,.tsx,.js,.jsx,.json'",
    "compile:watch": "yarn compile --watch",
    "clean": "rm -rf lib && rm -f tsconfig.build.tsbuildinfo && rm -rf node_modules"
  },
  "devScripts": [
    "compile:watch",
    "definition:watch"
  ],
  "dependencies": {
    "@azzapp/data": "workspace:^",
    "@azzapp/i18n": "workspace:^",
    "@azzapp/payment": "workspace:^",
    "@azzapp/shared": "workspace:^",
    "@graphql-tools/schema": "^10.0.13",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sentry/nextjs": "^7.113.0",
    "dataloader": "^2.2.3",
    "expiry-map": "^2.0.0",
    "graphql": "^16.10.0",
    "graphql-middleware": "^6.1.35",
    "graphql-relay": "^0.10.2",
    "graphql-scalars": "^1.24.0",
    "graphql-shield": "^7.6.5",
    "graphql-tag": "^2.12.6",
    "latinize": "^2.0.0",
    "lodash": "^4.17.21",
    "pexels": "^1.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/latinize": "^0.2.18",
    "@types/lodash": "^4.17.13",
    "eslint": "^9.17.0",
    "typescript": "^5.7.2"
  }
}

    "NSCameraUsageDescription" : {
      "comment" : "Privacy - Camera Usage Description",
      "extractionState" : "extracted_with_value",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "azzapp benötigt Zugriff auf deine Kamera, um Fotos und Videos aufzunehmen und in der App zu teilen."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "new",

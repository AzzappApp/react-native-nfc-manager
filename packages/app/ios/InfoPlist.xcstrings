      }
    },
    "NSContactsUsageDescription" : {
      "comment" : "Privacy - Contacts Usage Description",
      "extractionState" : "extracted_with_value",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Erlaube azzapp den Zugriff auf deine Kontakte, um neue Verbindungen schnell in deinem Telefonbuch zu speichern oder die Multi-User-Lösung für dein Unternehmen zu nutzen."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "new",
            "value" : "Allow azzapp to access your contacts to quickly add new connections to your phone book, or use the Multi-User solution for your company."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Permite que azzapp acceda a tus contactos para añadir rápidamente nuevas conexiones a tu agenda o utilizar la solución Multi-Usuario para tu empresa."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Autorisez azzapp à accéder à vos contacts pour ajouter rapidement de nouvelles connexions à votre répertoire, ou pour utiliser la fonction Multi-Utilisateur pour votre compagnie."
          }
        },
        "it" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Consenti ad azzapp di accedere ai tuoi contatti per aggiungere rapidamente nuove connessioni alla rubrica o utilizzare la soluzione Multi-Utente per la tua azienda."
          }
        }
      }
    },
    "NSLocationWhenInUseUsageDescription" : {
      "comment" : "Privacy - Location when in use Usage Description",
      "extractionState" : "manual",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Azzapp verwendet deinen Standort, um zu speichern, wo du Kontaktdaten ausgetauscht hast, damit du später leichter wieder Kontakt aufnehmen kannst."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Azzapp uses your location to save where you exchanged contact information, making it easy to reconnect later."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Azzapp usa tu ubicación para guardar dónde intercambiaste información de contacto, facilitando volver a conectar más tarde."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "azzapp utilise votre position pour enregistrer où vous étiez lorsque vous avez échangé vos coordonnées, de manière à retrouver vos contacts plus facilement."
          }
        },
        "it" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Azzapp utilizza la tua posizione per salvare dove hai scambiato i contatti, rendendo più facile riconnettersi in seguito."
          }
        }
      }
    },

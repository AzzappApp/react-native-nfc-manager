{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "paths": {
      "@azzapp/shared/*": ["../shared/src/*"],
      "#*": ["./src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", "**/*.json"],
  "exclude": [
    ".bundle",
    "android",
    "assets",
    "dist",
    "lib",
    "fastlane",
    "ios",
    "node_modules",
    "vendor"
  ]
}

{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "paths": {
      "@azzapp/shared/*": ["../shared/src/*"],
      "#*": ["./src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.json",
    "../shared/src/CountryFlag.ts"
  ],
  "exclude": [
    ".bundle",
    "android",
    "native_modules",
    "assets",
    "dist",
    "lib",
    "fastlane",
    "ios",
    "node_modules",
    "vendor",
    "tsconfig.json"
  ]
}

{
  "name": "@azzapp/app",
  "version": "0.0.1",
  "scripts": {
    "build:lib": "yarn lib:clean && yarn lib:compile && yarn lib:definition && yarn assets:copy",
    "build:ios": "bundle exec fastlane ios sign",
    "build:ios:native": "SKIP_BUNDLING=true bundle exec fastlane ios build",
    "build:ios:bundle": "./scripts/bundle-ios.sh",
    "build:android": "bundle exec fastlane android build",
    "typecheck": "tsc --noEmit",
    "rn:watch": "react-native start",
    "run:android": "react-native run-android --no-packager",
    "run:ios": "react-native run-ios --no-packager",
    "deploy:ios": "bundle exec fastlane ios deploy",
    "deploy:android": "bundle exec fastlane android deploy",
    "lib:clean": "rimraf lib",
    "lib:definition": "tsc -p tsconfig.lib.json",
    "lib:definition:watch": "yarn lib:definition --watch",
    "lib:compile": "babel src --config-file ./babel.config.lib.js -d lib --extensions '.ts,.tsx,.js,.jsx,.json'",
    "lib:compile:watch": "yarn lib:compile --watch",
    "assets:copy": "node scripts/copy-assets",
    "assets:watch": "yarn assets:copy && node scripts/watch-assets",
    "pod-install": "cd ios && bundle exec pod install"
  },
  "devScripts": [
    "lib:compile:watch",
    "lib:definition:watch",
    "assets:watch",
    "rn:watch"
  ],
  "dependencies": {
    "@azzapp/i18n": "*",
    "@azzapp/relay": "*",
    "@azzapp/shared": "*",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-camera-roll/camera-roll": "^5.2.0",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "chroma-js": "^2.4.2",
    "events": "^3.3.0",
    "graphql": "^16.6.0",
    "libphonenumber-js": "^1.10.18",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-intl": "^6.2.5",
    "react-native": "0.70.6",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "2.9.0",
    "react-native-linear-gradient": "2.6.2",
    "react-native-localize": "^2.2.4",
    "react-native-mmkv": "2.5.1",
    "react-native-pager-view": "6.1.2",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "2.14.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "3.18.2",
    "react-native-svg": "13.6.0",
    "react-native-tab-view": "3.3.4",
    "react-native-view-shot": "^3.5.0",
    "react-native-vision-camera": "^2.15.2",
    "react-native-web": "0.18.10",
    "react-relay": "^14.1.0",
    "react-transition-group": "^4.4.5",
    "use-debounce": "9.0.2"
  },
  "devDependencies": {
    "@testing-library/react-native": "^11.5.0",
    "@types/chroma-js": "^2.1.4",
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.70.8",
    "@types/react-relay": "^14.1.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-transition-group": "^4.4.5",
    "@types/relay-test-utils": "^14.1.0",
    "copyfiles": "^2.4.1",
    "fb-watchman": "^2.0.2",
    "react-native-dotenv": "^3.4.6",
    "react-native-monorepo-tools": "^1.2.1",
    "react-test-renderer": "^18.2.0",
    "relay-test-utils": "^14.1.0",
    "rimraf": "^4.1.2"
  },
  "jest": {
    "preset": "react-native"
  }
}

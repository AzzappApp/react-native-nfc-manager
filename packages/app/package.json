{
  "name": "@azzapp/app",
  "version": "0.0.4",
  "scripts": {
    "build:lib": "yarn lib:clean && yarn lib:compile && yarn lib:definition && yarn assets:copy",
    "build:ios": "bundle exec fastlane ios sign",
    "build:ios:native": "SKIP_BUNDLING=true bundle exec fastlane ios build",
    "build:ios:bundle": "./scripts/bundle-ios.sh",
    "build:android": "bundle exec fastlane android build",
    "typecheck": "tsc --noEmit",
    "rn:watch": "react-native start",
    "run:android": "react-native run-android --no-packager",
    "run:ios": "react-native run-ios --no-packager",
    "deploy:ios": "bundle exec fastlane ios deploy",
    "deploy:android": "bundle exec fastlane android deploy",
    "lib:clean": "rimraf lib",
    "lib:definition": "tsc -p tsconfig.lib.json",
    "lib:definition:watch": "yarn lib:definition --watch",
    "lib:compile": "babel src --config-file ./babel.config.lib.js -d lib --extensions '.ts,.tsx,.js,.jsx'",
    "lib:compile:watch": "yarn lib:compile --watch",
    "assets:copy": "node scripts/copy-assets",
    "assets:watch": "yarn assets:copy && node scripts/watch-assets",
    "pod-install": "cd ios && bundle exec pod install",
    "clean": "watchman watch-del-all && rm -Rf lib && rm -Rf node_modules &&  rm -Rf ios/Pods && rm -Rf ios/build  && cd android && ./gradlew clean"
  },
  "devScripts": [
    "lib:compile:watch",
    "lib:definition:watch",
    "assets:watch",
    "rn:watch"
  ],
  "dependencies": {
    "@azzapp/i18n": "*",
    "@azzapp/relay": "*",
    "@azzapp/shared": "*",
    "@formatjs/intl-locale": "^3.3.0",
    "@formatjs/intl-relativetimeformat": "^11.2.2",
    "@formatjs/intl-pluralrules": "^5.2.2",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-camera-roll/camera-roll": "^5.3.1",
    "@react-native-clipboard/clipboard": "^1.8.1",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "chroma-js": "^2.4.2",
    "events": "^3.3.0",
    "graphql": "^16.6.0",
    "i18n-iso-countries": "^7.5.0",
    "libphonenumber-js": "^1.10.24",
    "lodash": "^4.17.21",
    "lottie-react-native": "^6.0.0-rc.6",
    "react": "^18.2.0",
    "react-intl": "^6.3.2",
    "react-native": "0.71.5",
    "react-native-haptic-feedback": "^2.0.3",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "2.9.0",
    "react-native-linear-gradient": "2.6.2",
    "react-native-localize": "^2.2.6",
    "react-native-mime-types": "^2.4.0",
    "react-native-mmkv": "^2.6.3",
    "react-native-pager-view": "^6.2.0",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "^2.17.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-view-shot": "^3.5.0",
    "react-native-vision-camera": "^2.15.4",
    "react-native-web": "^0.19.1",
    "react-native-email-link": "^1.14.5",
    "react-phone-number-input": "^3.2.19",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-relay": "^14.1.0",
    "react-transition-group": "^4.4.5",
    "use-debounce": "^9.0.3",
    "react-hook-form": "^7.43.9",
    "@hookform/resolvers": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.0.1",
    "@types/chroma-js": "^2.4.0",
    "@types/react": "^18.0.31",
    "@types/react-relay": "^14.1.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-transition-group": "^4.4.5",
    "@types/relay-test-utils": "^14.1.0",
    "copyfiles": "^2.4.1",
    "fb-watchman": "^2.0.2",
    "react-native-dotenv": "^3.4.8",
    "react-native-monorepo-tools": "^1.2.1",
    "react-test-renderer": "^18.2.0",
    "relay-test-utils": "^14.1.0",
    "rimraf": "^4.1.2"
  },
  "jest": {
    "preset": "react-native"
  }
}

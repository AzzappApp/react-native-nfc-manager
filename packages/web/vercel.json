{
  "git": {
    "deploymentEnabled": false
  },
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/cancelPayments",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/unpublishWebCards",
      "schedule": "*/3 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "https://storefront.azzapp.com/"
    },
    {
      "source": "/plus/:match*",
      "destination": "https://storefront.azzapp.com/plus/:match*"
    },
    {
      "source": "/personal/:match*",
      "destination": "https://storefront.azzapp.com/personal/:match*"
    },
    {
      "source": "/pricing",
      "destination": "https://storefront.azzapp.com/pricing"
    },
    {
      "source": "/company/:match*",
      "destination": "https://storefront.azzapp.com/company/:match*"
    },
    {
      "source": "/legal/:match*",
      "destination": "https://storefront.azzapp.com/legal/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/_framer/:match*",
      "destination": "https://storefront.azzapp.com/_framer/:match*",
      "permanent": true
    }
  ]
}

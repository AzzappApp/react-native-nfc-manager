{
  "git": {
    "deploymentEnabled": false
  },
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "https://storefront.azzapp.com/" },
    {
      "source": "/plus/:match*",
      "destination": "https://storefront.azzapp.com/plus/:match*"
    },
    {
      "source": "/personal/:match*",
      "destination": "https://storefront.azzapp.com/personal/:match*"
    },
    {
      "source": "/pricing",
      "destination": "https://storefront.azzapp.com/pricing"
    },
    {
      "source": "/company/:match*",
      "destination": "https://storefront.azzapp.com/company/:match*"
    },
    {
      "source": "/legal/:match*",
      "destination": "https://storefront.azzapp.com/legal/:match*"
    },
    {
      "source": "/_framer/login/",
      "destination": "https://storefront.azzapp.com/_framer/login/"
    }
  ]
}

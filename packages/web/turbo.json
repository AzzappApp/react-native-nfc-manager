{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      "dependsOn": ["//#relay:build", "^build", "@azzapp/app#build:lib"],
      "inputs": ["src/**", "src/public/**"],
      "outputs": [".next/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["//#relay:build", "^build", "@azzapp/app#build:lib"]
    }
  }
}

{
  "name": "@azzapp/monorepo",
  "version": "1.4.0-canary.18",
  "private": true,
  "packageManager": "yarn@4.5.0",
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/app/native_modules/*"
    ]
  },
  "scripts": {
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "build": "turbo run build",
    "build-app-dependencies": "turbo run app:build --filter=./packages/app^...",
    "dev": "node ./scripts/dev.js",
    "build:ios": "turbo run build:ios",
    "build:android": "turbo run build:android",
    "graphql-codegen": "graphql-codegen",
    "relay:build": "relay-compiler",
    "relay:watch": "relay-compiler --watch ./relay.config.dev.js",
    "relay:prepare": "touch ./packages/web/src/persisted-query-map.json",
    "i18n:extract": "yarn workspace @azzapp/i18n extract",
    "i18n:pull": "yarn workspace @azzapp/i18n pull",
    "i18n:purge": "yarn workspace @azzapp/i18n purge",
    "run:ios": "yarn workspace @azzapp/app run:ios",
    "run:android": "yarn workspace @azzapp/app run:android",
    "update-graphql-schema": "yarn graphql-codegen",
    "clean": "turbo run clean && rm -Rf node_modules && rm -f tsconfig.tsbuildinfo",
    "release": "node ./scripts/release.js",
    "bump-version": "node ./scripts/bumpVersion.js",
    "write-dev-changelog": "node ./scripts/writeDevChangeLog.js",
    "persist-queries": "yarn workspace @azzapp/web persist-queries"
  },
  "devScripts": [
    "relay:watch",
    "graphql-codegen --watch"
  ],
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-jsx": "^7.25.7",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/plugin-transform-react-jsx": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/runtime": "^7.25.7",
    "@eslint/compat": "^1.2.0",
    "@formatjs/cli-lib": "^6.4.2",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.1.0",
    "@graphql-codegen/typescript-resolvers": "^4.3.0",
    "@graphql-tools/executor-http": "^1.1.6",
    "@octokit/rest": "^20.1.1",
    "@parcel/watcher": "^2.4.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-formatjs": "^10.5.16",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-relay": "^18.1.0",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.4.4",
    "csv-stringify": "^6.5.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-formatjs": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "git-spawned-stream": "^1.0.1",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react-native-version": "^4.0.0",
    "relay-compiler": "^18.1.0",
    "semver": "^7.6.3",
    "tagged-versions": "^1.3.0",
    "ts-jest": "^29.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.6.2"
  },
  "resolutions": {
    "graphql": "^16.8.0",
    "node-pty": "1.0.0",
    "cliui": "^8.0.1",
    "@envelop/sentry@^9.0.0": "patch:@envelop/sentry@npm%3A9.0.0#./.yarn/patches/@envelop-sentry-npm-9.0.0-d01d6a8911.patch",

    "expo-contacts@^13.0.5": "patch:expo-contacts@npm%3A13.0.5#./.yarn/patches/expo-contacts-npm-13.0.5-ee92319df7.patch",
    "vcard-creator@^0.7.0": "patch:vcard-creator@npm%3A0.7.0#./.yarn/patches/vcard-creator-npm-0.7.0-a36000c711.patch",
    "@shopify/react-native-skia@^1.3.13": "patch:@shopify/react-native-skia@npm%3A1.3.13#./.yarn/patches/@shopify-react-native-skia-npm-1.3.13-cfaf8220a9.patch",
    "@react-native-camera-roll/camera-roll@^7.8.3": "patch:@react-native-camera-roll/camera-roll@npm%3A7.8.3#./.yarn/patches/@react-native-camera-roll-camera-roll-npm-7.8.3-9c4439f311.patch",

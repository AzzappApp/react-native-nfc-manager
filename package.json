{
  "name": "@azzapp/monorepo",
  "version": "0.5.5-canary.231",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "postinstall": "patch-package",
    "lint": "eslint .",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "build": "turbo run build",
    "build-app-dependencies": "turbo run build --filter=app^...--filter=payment^...",
    "dev": "node ./scripts/dev.js",
    "build:ios": "turbo run build:ios",
    "build:android": "turbo run build:android",
    "graphql-codegen": "graphql-codegen",
    "relay:build": "relay-compiler",
    "relay:watch": "relay-compiler --watch ./relay.config.dev.js",
    "relay:prepare": "touch ./packages/web/src/persisted-query-map.json",
    "i18n:extract": "yarn workspace @azzapp/i18n extract",
    "i18n:pull": "yarn workspace @azzapp/i18n pull",
    "i18n:push": "yarn workspace @azzapp/i18n push",
    "run:ios": "yarn workspace @azzapp/app run:ios",
    "run:android": "yarn workspace @azzapp/app run:android",
    "update-graphql-schema": "yarn graphql-codegen",
    "clean": "yarn workspaces run clean && rm -Rf node_modules && rm -f tsconfig.tsbuildinfo",
    "release": "node ./scripts/release.js",
    "bump-version": "node ./scripts/bumpVersion.js",
    "write-dev-changelog": "node ./scripts/writeDevChangeLog.js",
    "persist-queries": "yarn workspace @azzapp/web persist-queries",
    "payment-api-client": "node ./scripts/generatePaymentApiClient.js"
  },
  "devScripts": [
    "relay:watch",
    "graphql-codegen --watch"
  ],
  "devDependencies": {
    "@babel/cli": "7.24.1",
    "@babel/core": "7.24.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-jsx": "^7.24.1",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "7.24.1",
    "@babel/runtime": "^7.24.4",
    "@babel/plugin-transform-modules-commonjs": "7.24.1",
    "@formatjs/cli-lib": "^6.3.8",
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@graphql-tools/executor-http": "1.0.9",
    "@octokit/rest": "^20.0.2",
    "@parcel/watcher": "^2.4.1",
    "@react-native/babel-preset": "^0.75.0-main",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/blessed": "^0.1.21",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-formatjs": "^10.5.14",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-relay": "^16.2.0",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.4.4",
    "csv-stringify": "^6.4.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-formatjs": "^4.12.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-spawned-stream": "^1.0.1",
    "jest": "^29.7.0",
    "jest-expo": "^50.0.4",
    "openapi-typescript": "^6.7.5",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "react-native-version": "^4.0.0",
    "relay-compiler": "^16.2.0",
    "semver": "^7.6.0",
    "tagged-versions": "^1.3.0",
    "ts-jest": "^29.1.2",
    "turbo": "^1.13.2",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "@types/react": "^18.2.67",
    "zod": "3.22.4",
    "graphql": "^16.8.0",
    "node-pty": "1.0.0",
    "cliui": "^8.0.1"
  }
}

{
  "name": "@azzapp/monorepo",
  "version": "0.5.19-canary.134",
  "private": true,
  "packageManager": "yarn@3.8.2",
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/app/native_modules/*"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "build": "turbo run build",
    "build-app-dependencies": "turbo run app:build --filter=./packages/app^...",
    "dev": "node ./scripts/dev.js",
    "build:ios": "turbo run build:ios",
    "build:android": "turbo run build:android",
    "graphql-codegen": "graphql-codegen",
    "relay:build": "relay-compiler",
    "relay:watch": "relay-compiler --watch ./relay.config.dev.js",
    "relay:prepare": "touch ./packages/web/src/persisted-query-map.json",
    "i18n:extract": "yarn workspace @azzapp/i18n extract",
    "i18n:pull": "yarn workspace @azzapp/i18n pull",
    "i18n:push": "yarn workspace @azzapp/i18n push",
    "run:ios": "yarn workspace @azzapp/app run:ios",
    "run:android": "yarn workspace @azzapp/app run:android",
    "update-graphql-schema": "yarn graphql-codegen",
    "clean": "turbo run clean && rm -Rf node_modules && rm -f tsconfig.tsbuildinfo",
    "release": "node ./scripts/release.js",
    "bump-version": "node ./scripts/bumpVersion.js",
    "write-dev-changelog": "node ./scripts/writeDevChangeLog.js",
    "persist-queries": "yarn workspace @azzapp/web persist-queries"
  },
  "devScripts": [
    "relay:watch",
    "graphql-codegen --watch"
  ],
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-jsx": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "7.24.7",
    "@babel/plugin-transform-react-jsx": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@babel/runtime": "^7.24.7",
    "@formatjs/cli-lib": "^6.4.0",
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.0.2",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-resolvers": "^4.1.0",
    "@graphql-tools/executor-http": "1.0.9",
    "@octokit/rest": "^20.1.1",
    "@parcel/watcher": "^2.4.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-formatjs": "^10.5.16",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-relay": "^17.0.0",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.4.4",
    "csv-stringify": "^6.4.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-formatjs": "^4.12.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-spawned-stream": "^1.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react-native-version": "^4.0.0",
    "relay-compiler": "^17.0.0",
    "semver": "^7.6.0",
    "tagged-versions": "^1.3.0",
    "ts-jest": "^29.1.2",
    "turbo": "latest",
    "typescript": "^5.5.3"
  },
  "resolutions": {
    "graphql": "^16.8.0",
    "node-pty": "1.0.0",
    "cliui": "^8.0.1",
    "@envelop/sentry@^9.0.0": "patch:@envelop/sentry@npm%3A9.0.0#./.yarn/patches/@envelop-sentry-npm-9.0.0-d01d6a8911.patch",
    "expo-contacts@~13.0.3": "patch:expo-contacts@npm%3A13.0.3#./.yarn/patches/expo-contacts-npm-13.0.3-c6435ab472.patch",
    "drizzle-kit@^0.22.7": "patch:drizzle-kit@npm%3A0.22.7#./.yarn/patches/drizzle-kit-npm-0.22.7-19553a88f0.patch",
    "drizzle-orm@^0.31.2": "patch:drizzle-orm@npm%3A0.31.2#./.yarn/patches/drizzle-orm-npm-0.31.2-f0b4701b98.patch",
    "react-native-screens@^3.32.0": "patch:react-native-screens@npm%3A3.32.0#./.yarn/patches/react-native-screens-npm-3.32.0-a22eba53a2.patch",
    "@shopify/react-native-skia@1.3.8": "patch:@shopify/react-native-skia@npm%3A1.3.8#./.yarn/patches/@shopify-react-native-skia-npm-1.3.8-0ad031190d.patch"
  },
  "dependencies": {
    "node-gyp": "^10.1.0"
  }
}

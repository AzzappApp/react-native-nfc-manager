{
  "name": "@azzapp/monorepo",
  "version": "0.1.3-canary.8",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "husky install",
    "lint": "eslint .",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "build": "turbo run build",
    "build-app-dependencies": "turbo run build --filter=app^...",
    "dev": "node ./scripts/dev.js",
    "build:ios": "turbo run build:ios",
    "build:android": "turbo run build:android",
    "graphql-codegen": "graphql-codegen",
    "relay:build": "relay-compiler",
    "relay:watch": "relay-compiler --watch",
    "relay:prepare": "touch ./packages/relay/query-map.json",
    "i18n:extract": "node scripts/i18nextract.js",
    "deploy:ios": "yarn workspace @azzapp/app deploy:ios",
    "deploy:android": "yarn workspace @azzapp/app deploy:android",
    "run:ios": "yarn workspace @azzapp/app run:ios",
    "run:android": "yarn workspace @azzapp/app run:android",
    "update-graphql-schema": "yarn graphql-codegen",
    "clean": "yarn workspaces run clean && rm -Rf node_mdules && rm -f tsconfig.tsbuildinfo",
    "release": "node ./scripts/release.js",
    "bump-version": "node ./scripts/bumpVersion.js",
    "write-dev-changelog": "node ./scripts/writeDevChangeLog.js"
  },
  "devScripts": [
    "relay:watch",
    "graphql-codegen --watch"
  ],
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@formatjs/cli-lib": "^6.1.3",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.0",
    "@octokit/rest": "^19.0.13",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/blessed": "^0.1.21",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-formatjs": "^10.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-relay": "^14.1.0",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.4.4",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-formatjs": "^4.9.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fix-esm": "^1.0.1",
    "git-spawned-stream": "^1.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.73.9",
    "node-fetch": "2",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.7",
    "react-native-version": "^4.0.0",
    "relay-compiler": "^14.1.0",
    "semver": "^7.5.3",
    "tagged-versions": "^1.3.0",
    "ts-jest": "^29.1.0",
    "turbo": "^1.8.8",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "@types/react": "18.2.12",
    "graphql": "16.6.0"
  }
}

{
  "name": "@azzapp/monorepo",
  "version": "0.5.15-canary.2",
  "private": true,
  "packageManager": "yarn@3.6.4",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "build": "turbo run build",
    "build-app-dependencies": "turbo run app:build --filter=app^...",
    "dev": "node ./scripts/dev.js",
    "build:ios": "turbo run build:ios",
    "build:android": "turbo run build:android",
    "graphql-codegen": "graphql-codegen",
    "relay:build": "relay-compiler",
    "relay:watch": "relay-compiler --watch ./relay.config.dev.js",
    "relay:prepare": "touch ./packages/web/src/persisted-query-map.json",
    "i18n:extract": "yarn workspace @azzapp/i18n extract",
    "i18n:pull": "yarn workspace @azzapp/i18n pull",
    "i18n:push": "yarn workspace @azzapp/i18n push",
    "run:ios": "yarn workspace @azzapp/app run:ios",
    "run:android": "yarn workspace @azzapp/app run:android",
    "update-graphql-schema": "yarn graphql-codegen",
    "clean": "yarn workspaces run clean && rm -Rf node_modules && rm -f tsconfig.tsbuildinfo",
    "release": "node ./scripts/release.js",
    "bump-version": "node ./scripts/bumpVersion.js",
    "write-dev-changelog": "node ./scripts/writeDevChangeLog.js",
    "persist-queries": "yarn workspace @azzapp/web persist-queries",
    "payment-api-client": "touch .env && node --env-file=.env ./scripts/generatePaymentApiClient.js"
  },
  "devScripts": [
    "relay:watch",
    "graphql-codegen --watch"
  ],
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-jsx": "^7.24.1",
    "@babel/plugin-transform-modules-commonjs": "7.24.1",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "7.24.1",
    "@babel/runtime": "^7.24.5",
    "@formatjs/cli-lib": "^6.4.0",
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/schema-ast": "^4.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@graphql-tools/executor-http": "1.0.9",
    "@octokit/rest": "^20.1.1",
    "@parcel/watcher": "^2.4.1",
    "@react-native/babel-preset": "0.74.83",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/blessed": "^0.1.21",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-formatjs": "^10.5.14",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-relay": "^16.2.0",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.4.4",
    "csv-stringify": "^6.4.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-formatjs": "^4.12.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-spawned-stream": "^1.0.1",
    "jest": "^29.7.0",
    "openapi-typescript": "^6.7.5",
    "prettier": "^3.2.5",
    "react-native-version": "^4.0.0",
    "relay-compiler": "^16.2.0",
    "semver": "^7.6.0",
    "tagged-versions": "^1.3.0",
    "ts-jest": "^29.1.2",
    "turbo": "^1.13.3",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "graphql": "^16.8.0",
    "node-pty": "1.0.0",
    "cliui": "^8.0.1",
    "@envelop/sentry@^9.0.0": "patch:@envelop/sentry@npm%3A9.0.0#./.yarn/patches/@envelop-sentry-npm-9.0.0-d01d6a8911.patch",
    "drizzle-kit@0.20.14": "patch:drizzle-kit@npm%3A0.20.14#./.yarn/patches/drizzle-kit-npm-0.20.14-cf0918f896.patch",
    "drizzle-orm@^0.30.8": "patch:drizzle-orm@npm%3A0.30.8#./.yarn/patches/drizzle-orm-npm-0.30.8-d3cedab5ef.patch",
    "react-native-screens@^3.31.1": "patch:react-native-screens@npm%3A3.31.1#./.yarn/patches/react-native-screens-npm-3.31.1-d5786be485.patch",
    "expo-contacts@~13.0.3": "patch:expo-contacts@npm%3A13.0.3#./.yarn/patches/expo-contacts-npm-13.0.3-c6435ab472.patch",
    "react-native-vision-camera@^4.0.5": "patch:react-native-vision-camera@npm%3A4.0.5#./.yarn/patches/react-native-vision-camera-npm-4.0.5-0f2bdc9daa.patch"
  },
  "dependencies": {
    "node-gyp": "^10.1.0"
  }
}
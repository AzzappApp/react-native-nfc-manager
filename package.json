{
  "name": "@azzapp/monorepo",
  "version": "0.5.1-canary.8",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "postinstall": "patch-package",
    "lint": "eslint .",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "build": "turbo run build",
    "build-app-dependencies": "turbo run build --filter=app^...",
    "dev": "node ./scripts/dev.js",
    "build:ios": "turbo run build:ios",
    "build:android": "turbo run build:android",
    "graphql-codegen": "graphql-codegen",
    "relay:build": "relay-compiler",
    "relay:watch": "relay-compiler --watch ./relay.config.dev.js",
    "relay:prepare": "touch ./packages/relay/query-map.json",
    "i18n:extract": "node scripts/i18nextract.js",
    "deploy:ios": "yarn workspace @azzapp/app deploy:ios",
    "deploy:android": "yarn workspace @azzapp/app deploy:android",
    "run:ios": "yarn workspace @azzapp/app run:ios",
    "run:android": "yarn workspace @azzapp/app run:android",
    "update-graphql-schema": "yarn graphql-codegen",
    "clean": "yarn workspaces run clean && rm -Rf node_mdules && rm -f tsconfig.tsbuildinfo",
    "release": "node ./scripts/release.js",
    "bump-version": "node ./scripts/bumpVersion.js",
    "write-dev-changelog": "node ./scripts/writeDevChangeLog.js"
  },
  "devScripts": [
    "relay:watch",
    "graphql-codegen --watch"
  ],
  "devDependencies": {
    "@babel/cli": "7.22.10",
    "@babel/core": "7.22.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "7.22.5",
    "@babel/runtime": "^7.22.10",
    "@formatjs/cli-lib": "^6.1.3",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@octokit/rest": "^20.0.1",
    "@parcel/watcher": "^2.3.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/blessed": "^0.1.21",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-formatjs": "^10.5.3",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-relay": "^14.1.0",
    "blessed": "^0.1.81",
    "blessed-xterm": "^1.4.4",
    "csv-stringify": "^6.4.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-formatjs": "^4.10.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-spawned-stream": "^1.0.1",
    "jest": "^29.6.3",
    "jest-expo": "^49.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "node-fetch": "^2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.2",
    "react-native-version": "^4.0.0",
    "relay-compiler": "^14.1.0",
    "semver": "^7.5.4",
    "tagged-versions": "^1.3.0",
    "ts-jest": "^29.1.1",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "@types/react": "^18.2.20",
    "graphql": "^16.8.0",
    "node-pty": "1.0.0"
  }
}
